    <script type="text/javascript">
$('.report-broken-link').click(function(event) {
    event.preventDefault();
    
    $('.broken-link-form').show(); 
    
    var ref = document.referrer;
    if(!document.referrer) { ref = "Direct input"; }
    
    $('#current_url').val(ref);
    $('#broken_url').val(document.location.protocol + '//' + document.location.hostname + document.location.pathname); 
    $(this).hide(); 
});
</script>    
